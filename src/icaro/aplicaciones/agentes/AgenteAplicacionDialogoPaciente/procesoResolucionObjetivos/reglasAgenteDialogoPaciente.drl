 when
 	notif:NotificacionPaciente(identInterlc:identNotificador, tipoNotificacion == tipoNotifPaciente.fecha,  msgg:mensajeNotificacion );
 	nom:ObtenerFechaCita(state == Objetivo.PENDING, objectReferenceId == identInterlc)
